<?xml version="1.0" encoding='UTF-8'?>
<odoo>
    <record model="ir.cron" id="partner_contact_birthday_wishes_cron">
		<field name="name">Contacts Birthday Wishes </field>
		<field name="model_id" ref="base.model_res_partner"/>
		<field name="interval_number">1</field>
		<field name="interval_type">days</field>
		<field name="numbercall">-1</field>
		<field name="doall" eval="False"/>
		<field name="nextcall" eval="datetime.now(pytz.timezone('UTC')).strftime('%Y-%m-%d 08:00:00')"/>
		<field name="state">code</field>
		<field name="code">model.mail_reminder()</field>
	</record>
</odoo>